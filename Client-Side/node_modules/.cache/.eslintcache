              </label>\r\n                    <input\r\n                      className=\"form__input\"\r\n                      id=\"password\"\r\n                      type=\"password\"\r\n                      placeholder=\"••••••••\"\r\n                      required=\"required\"\r\n                      minLength={8}\r\n                      ref={passwordRef}\r\n                    />\r\n                  </div>\r\n                  <div className=\"form__group right\">\r\n                    <button\r\n                      className=\"btn btn--small btn--green\"\r\n                      onClick={loginClickHandeler}\r\n                    >\r\n                      Login\r\n                    </button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n              <div className=\"line\">&nbsp;</div>\r\n            </div>\r\n            {/* {modalIsOpen && <Modal onCancel={closeModalHandler} onConfirm={closeModalHandler}/>}\r\n             {modalIsOpen && <Backdrop onCancel={closeModalHandler} />}   */}\r\n          </div>\r\n        </main>\r\n      </div>\r\n    </Layout>\r\n  );\r\n}\r\nexport default Login;\r\n","D:\\documents\\Semesters\\S7\\FYP\\client-side\\src\\components\\popup\\Modal.js",[],"D:\\documents\\Semesters\\S7\\FYP\\client-side\\src\\components\\popup\\Backdrop.js",[],"D:\\documents\\Semesters\\S7\\FYP\\client-side\\src\\pages\\Tours\\Overview.js",["66","67","68","69","70","71","72","73"],{"ruleId":"74","severity":1,"message":"75","line":4,"column":8,"nodeType":"76","messageId":"77","endLine":4,"endColumn":14},{"ruleId":"78","severity":1,"message":"79","line":47,"column":19,"nodeType":"80","endLine":47,"endColumn":31},{"ruleId":"78","severity":1,"message":"79","line":55,"column":19,"nodeType":"80","endLine":55,"endColumn":31},{"ruleId":"78","severity":1,"message":"79","line":63,"column":19,"nodeType":"80","endLine":63,"endColumn":31},{"ruleId":"78","severity":1,"message":"79","line":71,"column":19,"nodeType